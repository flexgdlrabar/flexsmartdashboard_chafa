{
  "type": "object",
  "comment": "Flex SMART Dashboard Information",
  "properties": {
    "Site": {
      "type": "string",
      "update": {
        "type": "none"
      }
    },
    "Building": {
      "type": "string",
      "update": {
        "type": "none"
      }
    },
    "isDashboard": {
      "type": "identifier",
      "selector": ".default-theme.des-mat",
      "control": "property.innerText"
    }
  },
  "connect": [
    {
      "match": [
        "isDashboard"
      ],
      "screens": [
        "dashboard"
      ]
    }
  ]
}
